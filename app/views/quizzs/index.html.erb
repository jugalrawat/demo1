<h1 style="color: #607e00">Listing quizzs</h1>
<br />
<table class="tsc_table_s8" width="100%">
  <thead>
    <tr>
      <th scope="col">Question</th>
      <th scope="col">Ans1</th>
      <th scope="col">Ans2</th>
      <th scope="col">Ans3</th>
      <th scope="col">Ans4</th>
      <th scope="col">Correctans</th>
      <th scope="col">...</th>
      <th scope="col">...</th>
      <th scope="col">...</th>
    </tr>
  </thead>
  <tfoot>
  <tr>
    <th scope="row"></th>
    <td colspan="7"></td>
  </tr>
  </tfoot>

    <% @quizzs.each do |quizz| %>
      <tr>
        <th scope="row"><%= quizz.question %></th>
        <td><%= quizz.ans1 %></td>
        <td><%= quizz.ans2 %></td>
        <td><%= quizz.ans3 %></td>
        <td><%= quizz.ans4 %></td>
        <td><%= quizz.correctans %></td>
        <td><%= link_to 'Show', quizz %></td>
        <td><%= link_to 'Edit', edit_quizz_path(quizz) %></td>
        <td><%= link_to 'Destroy', quizz, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>

</table>

<br>

<%= link_to 'New Quizz', new_quizz_path %>
